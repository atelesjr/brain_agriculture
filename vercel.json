{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
        "routes": [
          { "src": "^/producers(?:/.*)?$", "dest": "/producers.json" },
          { "handle": "filesystem" },
          { "src": "\/(.*)", "dest": "/index.html" }
        ]
}
