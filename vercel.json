{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
    "routes": [
      { "src": "^/producers(?:/.*)?$", "dest": "/api/producers" },
      { "handle": "filesystem" },
      { "src": "\/(.*)", "dest": "/index.html" }
    ]
}
