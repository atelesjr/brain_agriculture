{
	"name": "brain_agriculture",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"postbuild": "node scripts/copy-producers-to-dist.cjs",
		"lint": "eslint .",
		"preview": "vite preview",
		"test": "vitest --watch --reporter=verbose",
		"test:watch:stable": "vitest --watch --reporter=verbose",
		"test:run": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"test:local": "node ./scripts/run-tests-serial.cjs",
		"server": "json-server --watch server/db.json --port 3001",
		"dev:all": "concurrently \"npm run dev\" \"npm run server\"",
		"vercel-build": "vite build"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@reduxjs/toolkit": "^2.10.1",
		"json-server": "^1.0.0-beta.3",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-hook-form": "^7.66.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.9.5",
		"recharts": "^3.4.1",
		"styled-components": "^6.1.19",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@types/recharts": "^1.8.29",
		"@types/styled-components": "^5.1.35",
		"@vitejs/plugin-react": "^5.1.0",
		"@vitest/coverage-v8": "^4.0.8",
		"concurrently": "^9.2.1",
		"cross-env": "^7.0.3",
		"eslint": "^9.39.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"happy-dom": "^20.0.10",
		"jsdom": "^27.1.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.2",
		"vite-plugin-svgr": "^4.5.0",
		"vitest": "^4.0.8"
	},
	"engines": {
		"node": ">=18"
	}
}
